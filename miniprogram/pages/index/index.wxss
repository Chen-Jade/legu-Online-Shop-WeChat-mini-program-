page {
  background-color: #F3F4F5;
}
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  background-color: #fff;
  border-radius: 15rpx;
  justify-content: center;
  width: 90%;
  margin: 0 auto;
  margin-top: 30rpx;
  display: flex;
}
.index_cate navigator {
  padding: 15rpx 0;
  flex: 1;
  text-align: center;
}
.index_cate navigator .iconfont {
  color: var(--themeColor);
  font-size: 40rpx;
}
.index_floor {
  background-color: #fff;
  border-radius: 15rpx;
  justify-content: center;
  width: 90%;
  margin: 0 auto;
  margin-top: 30rpx;
}
.index_floor .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_title image {
  width: 100%;
}
.index_floor .floor_list {
  overflow: hidden;
}
.index_floor .floor_list navigator {
  float: left;
  width: 33.33%;
  /* 后四个超链接 */
  /* 2,3两个超链接 */
}
.index_floor .floor_list navigator:nth-last-child(-n+4) {
  /* 原图的宽高 232*386 */
  height: 25vw;
  border-left: 10rpx solid #ffffff;
}
.index_floor .floor_list navigator:nth-child(2),
.index_floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.index_floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}
